<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>protobuf-net.Reflection</AssemblyName>
    <Title>protobuf-net.Reflection</Title>
    <Description>ProtoBuf DSL (proto2 / proto3) and desccriptor tools for protobuf-net</Description>
    <TargetFrameworks>net40;netstandard1.3</TargetFrameworks><!--net20;net35;-->
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Configurations>Debug;Release;VS</Configurations>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\protobuf-net\protobuf-net.csproj" />
    <None Include="CodeGenerator.OneOfStub.cs" />
  </ItemGroup>
</Project>